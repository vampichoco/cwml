<?xml version="1.0" encoding="utf-8" ?>
<page>
  <head>
    <css>style.css</css>
  </head>
  <content>
    <query collection="files" presenceOf="@queryString/f">
      <condition>
        <equals name="filename" value="@queryString/f" />
      </condition>
      <output inherits="div">
        <rawImage>@file</rawImage>
      </output>
    </query>
  </content>
</page>
